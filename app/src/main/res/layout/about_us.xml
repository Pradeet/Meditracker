<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:weightSum="3" 
    >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical" >
        
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_margin="10dp"
            android:layout_weight="1.5"
            android:orientation="vertical"
            android:weightSum="3"
            android:background="#707883" >

            <TextView
                android:id="@+id/computer"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                android:layout_marginTop="20dp"
                android:drawablePadding="1dp"
                android:paddingBottom="10dp"
                android:paddingLeft="20dp"
                android:paddingRight="20dp"
                android:paddingTop="20dp"
                android:text="Project Team Details"
                android:textColor="#ffffff"
                android:textSize="20sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/pri"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="35dp"
                android:drawablePadding="1dp"
                android:text="1.Priyanka Damordar Dawande"
                android:textSize="15sp"
                android:textStyle="bold"
android:textColor="#ffffff"/>
            
            <TextView
                android:id="@+id/pr"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="35dp"
                android:drawablePadding="1dp"
                android:text="2.Pranali Prashant Jadhav"
                android:textSize="15sp"
                android:textStyle="bold"
                android:textColor="#ffffff" />
            
            <TextView
                android:id="@+id/mad"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="35dp"
                android:drawablePadding="1dp"
                android:text="3.Madhuri Santosh Nagori"
                android:textSize="15sp"
                android:textStyle="bold" 
                android:textColor="#ffffff"/>
             <TextView
                android:id="@+id/bg"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="35dp"
                android:drawablePadding="1dp"
                android:text="4.Bhagyesh Hemant Joshi"
                android:textSize="15sp"
                android:textStyle="bold" 
                android:textColor="#ffffff"/>
             <TextView
                android:id="@+id/ys"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="35dp"
                android:drawablePadding="1dp"
                android:text="5.Yash Sanjay Nikam"
                android:textSize="15sp"
                android:textStyle="bold" 
                android:textColor="#ffffff"/>
             <TextView
                android:id="@+id/as"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="35dp"
                android:drawablePadding="1dp"
                android:text="6.Ashish Pradip Mhalankar"
                android:textSize="15sp"
                android:textStyle="bold" 
                android:textColor="#ffffff"/>
       
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1.27"
        android:orientation="horizontal"
        android:weightSum="3" >

      <ImageView
          android:id="@+id/ku"
          android:layout_width="12dp"
          android:layout_height="fill_parent"
          android:layout_marginTop="20dp"
          android:layout_weight="3"
          android:background="@drawable/kumbha"
          android:drawablePadding="1dp"
          android:paddingBottom="20dp"
          android:paddingLeft="20dp"
          android:paddingRight="20dp"
          android:paddingTop="20dp"
          android:text="forth year"
          android:textSize="13sp"
          android:textStyle="bold" />
      
        
    </LinearLayout>

  <TextView
      android:id="@+id/call"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_alignBaseline="@+id/textView13"
      android:layout_alignBottom="@+id/textView13"
      android:layout_alignLeft="@+id/textView11"
      android:layout_gravity="center"
      android:autoLink="web"
      android:linksClickable="true"
      android:text="www.kumbha.org"
      android:textSize="15sp" />

    </LinearLayout>

    

</LinearLayout>